<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style>
        table,tr,td,th{
            border:1px,solid,black;
        }
    </style>
    <title>All Users</title>
</head>
<body>
    <table>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Username</th>            
        </tr>

    <% for(user of users){ %>
        <tr>
            <td><%= user.id %> </td>
            <td><%= user.email %> </td>
            <td><%= user.username %> </td>
            <td>
            <form action="/user/<%=user.id%>/edit" method="GET">
                 <button>Edit username</button>
            </form>
            </td>
        </tr>
    <% } %>
    </table>
</body>
</html>

